{
  "name": "affectation-ro",
  "displayName": "Affecation RO",
  "description": "Affecation Recherche Operationelle Min et Max",
  "author": {
    "name": "Zold",
    "email": "rakotonirinachriss@gmail.com"
  },
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sass": "^1.49.7",
    "wait-on": "^6.0.1",
    "web-vitals": "^1.0.1"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.minawk",
    "productName": "AffectationRO",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "installerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico",
      "deleteAppDataOnUninstall": true
    },
    "linux": {
      "target": "appImage"
    },
    "appImage": {
      "category": "education"
    },
    "files": [
      "**/*",
      "build/icon.*"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
  }
